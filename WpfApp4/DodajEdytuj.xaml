<Window x:Class="WpfApp4.DodajEdytuj"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp4"
        mc:Ignorable="d"
        Title="DodajEdytuj" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="textBoxImie" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10"/>
        <TextBox x:Name="textBoxNazwisko" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,40,0,0"/>
        <TextBox x:Name="textBoxPesel" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,80,0,0"/>
        <Button Content="Dodaj" Click="ButtonDodaj_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,120,0,0"/>
    </Grid>
</Window>
